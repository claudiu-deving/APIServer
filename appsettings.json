{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "Jwt": {
    "Key": "[JTW_KEY]",
    "Issuer": "ccsflow.com",
    "Audience": "YOUR_AUDIENCE",
    "DurationInMinutes": 120
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=[DATABASE_HOST_PW];Database=[DATABASE_PW];Username=[DATABASE_USER_PW];Password=[DATABASE_PASS_PW];Trust Server Certificate=True;"
  }
}
